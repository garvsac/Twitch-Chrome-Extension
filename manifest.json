{
    "manifest_version": 2,
    "name": "Tut",
    "version": "1.0",
    "description": "Halo!",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["marklet.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "./popup/index.html"
    }
}
